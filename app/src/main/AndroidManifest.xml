<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.josefco.repartosaa2">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.RepartosAA2"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".view.camiones.AddEditCamionView"
            android:exported="false" />
        <activity
            android:name=".view.camiones.SeeCamionDetallesView"
            android:exported="false" />
        <activity
            android:name=".view.conductores.AddEditConductorView"
            android:exported="false" />
        <activity
            android:name=".view.conductores.SeeConductorDetallesView"
            android:exported="false" />
        <activity
            android:name=".view.usuarios.SeeUsuarioDetallesView"
            android:exported="false" />
        <activity
            android:name=".view.usuarios.AddEditUsuarioView"
            android:exported="false" />
        <activity
            android:name=".view.paquetes.ListPaquetesView"
            android:exported="false" />
        <activity
            android:name=".view.usuarios.ListUsuariosView"
            android:exported="false" />
        <activity
            android:name=".view.camiones.ListCamionesView"
            android:exported="false" />
        <activity
            android:name=".view.conductores.ListConductoresView"
            android:exported="false" />
        <activity
            android:name=".view.InicioApp"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />
    </application>

</manifest>